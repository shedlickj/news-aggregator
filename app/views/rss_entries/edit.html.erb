<h1>Editing rss_entry</h1>

<% form_for(@rss_entry) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title, :disabled=>true %>
  </p>
  <p>
    <%= f.label :published %><br />
    <%= f.text_field :published, :disabled=>true %>
  </p>
  <p>
    <%= f.label :link %><br />
    <%= f.text_field :link, :disabled=>true %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description, :disabled=>true %>
  </p>
  <p>
    <%= f.label :data %><br />
    <%= f.text_field :data, :disabled=>true %>
  </p>
  <p>
    <%= f.label :favorite %><br />
    <%= f.check_box :favorite %>
  </p>
  <p>
    <%= f.label :hidden %><br />
    <%= f.check_box :hidden%>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @rss_entry %> |
<%= link_to 'Back', rss_entries_path %>